<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repository.rom.collection.browser"
        name="Rom Collection Browser Updates"
        version="1.0.0"
        provider-name="carabalb, jimyx17, malte, redkiller">
    <requires>
        <import addon="xbmc.addon" version="12.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository" name="Rom Collection Browser Updates">
        <info compressed="false">https://romcollectionbrowser.googlecode.com/svn/release/addons.xml</info>
        <checksum>https://romcollectionbrowser.googlecode.com/svn/release/addons.xml.md5</checksum>
        <datadir zip="true">https://romcollectionbrowser.googlecode.com/svn/release/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Rom Collection Browser Updates</summary>
        <description></description>
        <nofanart>true</nofanart>
        <platform>all</platform>
    </extension>
</addon>
